{
  "crons": [
    {
      "path": "/api/cron/update-saved-search-matches",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/send-saved-search-notifications",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "app/api/upload/route.ts": {
      "maxDuration": 30
    },
    "app/api/cron/update-saved-search-matches/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/send-saved-search-notifications/route.ts": {
      "maxDuration": 300
    }
  },
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}